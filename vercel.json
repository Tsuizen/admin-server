{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Contro-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "content-type"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "DELETE, PUT, POST, GET, OPTIONS"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/query-all-users"
    }
  ],
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ]
}
